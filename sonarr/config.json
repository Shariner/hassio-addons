{
  "name": "Sonarr",
  "version": "3.0.9.1549-ls154",
  "slug": "sonarr",
  "description": "Sonarr is a PVR for usenet and bittorrent users.",
  "url": "https://github.com/evilmarty/hassio-addons/tree/master/sonarr",
  "image": "ghcr.io/evilmarty/hassio-sonarr",
  "arch": [
    "amd64",
    "aarch64",
    "armhf"
  ],
  "startup": "application",
  "init": false,
  "boot": "auto",
  "services": [],
  "webui": "http://[HOST]:[PORT:8989]",
  "panel_icon": "mdi:selection-ellipse",
  "ingress": true,
  "ingress_stream": true,
  "map": [
    "share:rw",
    "ssl:ro",
    "media:rw"
  ],
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {
    "log_level": "error",
    "api_key": null
  },
  "schema": {
    "log_level": "list(info|warning|error|fatal|debug|trace)",
    "api_key": "str?"
  }
}
